@model List<Mogym.Application.Records.Suppliment.SupplimentRecord>
@{
    ViewBag.Title = "لیست مکمل ها";
    Layout = "_Layout";
}


<section class="content">
    <div class="container-fluid">
        <div class="row">

            <div class="col-12">
                <div class="card-body mb-50">
                    <a asp-action="Create" class="btn btn-success-card px-5 mb-4">ایجاد</a>

                    <table id="paidPlan" class="table table-bordered" style="width:100%">
                        <thead>
                        <tr>
                            <th>نام مکمل</th>
                        </tr>
                        </thead>
                        <tbody>
                        @foreach (var itm in Model)
                        {
                            <tr>

                                <td>@itm.Title</td>
                            </tr>
                        }
                        </tbody>
                    </table>

                </div>
                <!-- /.card-body -->
                <div class="card-footer">
                    <div class="form-group row">
                        <div class="col-md-9">
                            <p class="text-red text-right mt-2 font-weight-bold">@TempData["message"]</p>
                        </div>
                        <div class="form-actions-buttons col-md-12">
                            <a asp-action="Index" asp-controller="Account" class="btn btn-secondary py-2">بازگشت</a>
                        </div>
                    </div>
                </div>
                <!-- /.card -->
            </div>


        </div>
    </div>
</section>