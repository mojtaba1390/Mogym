@model List<Mogym.Application.Records.Profile.LastTrainersForHomePageRecord>

<!DOCTYPE html>
<html lang="en">


<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta property="og:title" content="موجیم-نسخه آزمایشی" />
    <meta property="og:type" content="website" />
    <meta property="og:image" content="http://mogym.ir/img/mogym-transparent.png" />
    <meta property="og:image:alt" content="موجیم تخصصی ترین مرجع مربیگری بدنسازی آنلاین" />
    <meta property="og:url" content="http://mogym.ir/" />




    <!--favicon icon-->
    <link rel="icon" href="~/img/favicon.ico" type="image/png" sizes="16x16">

    <!--title-->
    <title>موجیم-نسخه آزمایشی</title>

    <!--build:css-->
    <link rel="stylesheet" href="~/mainassets/css/main.css">
    <!-- endbuild -->
</head>

<body>

    <!--preloader start-->
    <div id="preloader">
        <div class="preloader-wrap">
            <img src="~/img/mogym-transparent.png" alt="mogym" class="img-fluid">
            <div class="thecube">
                <div class="cube c1"></div>
                <div class="cube c2"></div>
                <div class="cube c4"></div>
                <div class="cube c3"></div>
            </div>
        </div>
    </div>
    <!--preloader end-->
    <!--header section start-->
    <header class="header">
        <!--start navbar-->
        <nav class="navbar navbar-expand-lg fixed-top custom-nav">
            <div class="container">
                <a class="navbar-brand" asp-action="Index" asp-controller="Home">
                    <img src="~/img/mogym-transparent.png" alt="لوگو" class="img-fluid">
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="ناوبری را تغییر دهید">
                    <span class="ti-menu"></span>
                </button>

                <div class="collapse navbar-collapse h-auto" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto menu">
                        <li><a asp-controller="Account" asp-action="Index" class="page-scroll">خانه</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <!--header section end-->

    <div class="main">

        <!--hero section start-->
        <section class="position-relative overflow-hidden hero-section-3 ptb-100">
            <!--animated circle shape start-->
            <div class="animated-shape-wrap">
                <div class="animated-shape-item"></div>
                <div class="animated-shape-item"></div>
                <div class="animated-shape-item"></div>
                <div class="animated-shape-item"></div>
                <div class="animated-shape-item"></div>
            </div>
            <!--animated circle shape end-->
            <img src="~/mainassets/img/combined-shape.jpg" alt="s" class="shape-img-2">
            <img src="~/mainassets/img/shape-1.svg" alt="شکل گروه" width="100" class="img-fluid group-shape-1">
            <img src="~/mainassets/img/shape-2.svg" alt="شکل گروه" width="60" class="img-fluid group-shape-2">
            <div class="container">
                <div class="row align-items-center justify-content-between">
                    <div class="col-md-6 col-lg-6">
                        <div class="hero-content-left pt-5">
                            <h1 class="font-weight-bold">موجیم تخصصی ترین مرجع مربیگری بدنسازی آنلاین</h1>
                            <p class="lead">به وب سایت ما خوش آمدید. از اینکه مارو در بهتر شدن کمک می کنین از شما ممنونیم</p>
                            <div class="action-btns mt-4">
                                <ul class="list-inline">
                                    <li class="list-inline-item">
                                        <a asp-controller="Account" asp-action="Login" class="d-flex align-items-center app-download-btn btn btn-outline-brand-02 btn-rounded">
                                            <div class="download-text text-left">
                                                <h5 class="mb-0">ورود</h5>
                                            </div>
                                        </a>
                                    </li>
                                    <li class="list-inline-item">
                                        <a asp-controller="Account" asp-action="SignUp" class="d-flex align-items-center app-download-btn btn btn-outline-brand-02 btn-rounded">
                                            <div class="download-text text-left">
                                                <h5 class="mb-0">ثبت نام ورزشکار</h5>
                                            </div>
                                        </a>
                                    </li>
                                    <li class="list-inline-item">
                                        <a asp-controller="Account" asp-action="SignUpTrainer" class="d-flex align-items-center app-download-btn btn btn-outline-brand-02 btn-rounded">
                                            <div class="download-text text-left">
                                                <h5 class="mb-0"> ثبت نام مربی</h5>
                                            </div>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <div class="img-wrap">
                            <img src="~/mainassets/img/athletes.png" alt="ورزشکاران" class="img-fluid">
                        </div>
                    </div>
                </div>
            </div>

            <!--shape image start-->
            <img src="~/mainassets/img/hero-bg-shape-4.jpg" class="shape-image" alt="تصویر شکل">
            <!--shape image end-->
        </section>
        <!--hero section end-->
        <!--testimonial section start-->
        <section class="position-relative gradient-bg ptb-100">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-md-6 col-lg-5 mb-4 mb-sm-4 mb-md-0 mb-lg-0">
                        <div class="testimonial-heading text-white">
                            <h2 class="text-white">آخرین مربیان موجیم</h2>
                            <p>جای تجربه ی شما بین با تجربه های ما خالیه</p>
                        </div>
                        <div class="action-btns mt-3">
                            <a asp-controller="Home" asp-action="Trainers" class="btn btn-brand-02 mr-3">لیست تمام مربیان</a>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <div class="testimonial-content-wrap">
                            <div class="owl-carousel owl-theme client-testimonial-1 dot-indicator testimonial-shape">
                                @{
                                    foreach (var trainer in Model)
                                    {
                                        <div class="item">
                                            <div class="testimonial-quote-wrap">
                                                <div class="media author-info mb-3">
                                                    @*<div class="author-img mr-3">
                                                        @if (!string.IsNullOrWhiteSpace(trainer.ProfilePic))
                                                        {
                                                            <img src="/ProfilePic/@trainer.ProfilePic" alt="@trainer.FullName" class="img-fluid">
                                                        }
                                                        else
                                                        {
                                                            <img src="/img/avatar5.png" alt="@trainer.FullName" class="img-fluid">
                                                        }
                                                    </div>*@
                                                    <div class="media-body text-white">
                                                        <h5 class="mb-0 text-white">@trainer.FullName</h5> 
                                                        <span>@trainer.UserName</span>
                                                    </div>
                                                    <i class="fas fa-quote-right text-white"></i>
                                                </div>
                                                <div class="client-say text-white">
                                                    <p>@trainer.Biography</p>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                }

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--testimonial section end-->
        <!--promo section start-->
        <section class="promo-section ptb-100">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-md-10 col-lg-8">
                        <div class="section-heading text-center">
                            <h2>چرا موجیم متفاوته؟</h2>
                            <p></p>
                        </div>
                    </div>
                </div>
                <div class="row justify-content-md-center justify-content-sm-center">
                    <div class="col-md-6 col-lg-4">
                        <div class="card border-0 single-promo-card single-promo-hover text-center p-2 mt-4">
                            <div class="card-body">
                                <div class="pb-2">
                                    <span class="fas fa-cubes icon-size-lg color-primary"></span>
                                </div>
                                <div class="pt-2 pb-3">
                                    <h5>امکانات رایگان</h5>
                                    <p class="mb-0">خدمات ارائه شده ی این پلتفرم کاملا رایگان می باشد</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-4">
                        <div class="card border-0 single-promo-card single-promo-hover text-center p-2 mt-4">
                            <div class="card-body">
                                <div class="pb-2">
                                    <span class="fas fa-list icon-size-lg color-primary"></span>
                                </div>
                                <div class="pt-2 pb-3">
                                    <h5>فیلم آموزش حرکات</h5>
                                    <p class="mb-0">بیش از ۱۶۰ حرکت تمرینی با جزئیات و فیلم آموزشی</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-4">
                        <div class="card border-0 single-promo-card single-promo-hover text-center p-2 mt-4">
                            <div class="card-body">
                                <div class="pb-2">
                                    <span class="fas fa-save icon-size-lg color-primary"></span>
                                </div>
                                <div class="pt-2 pb-3">
                                    <h5>کاملا آنلاین</h5>
                                    <p class="mb-0">فرآیند کاملا آنلاین از انتخاب مربی تا دریافت برنامه</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--promo section end-->
        <section id="process" class="work-process-section position-relative  ptb-100 gray-light-bg">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-md-9 col-lg-8">
                        <div class="section-heading text-center mb-5">
                            <h2>فرآیند دریافت برنامه</h2>
                            <p>
                                ما تلاش کردیم ساده ترین راه و مطمئن ترین راه رو برای تناسب اندام شما طراحی کنیم
                            </p>
                        </div>
                    </div>
                </div>
                <div class="row align-items-center justify-content-md-center justify-content-sm-center">
                    <div class="col-md-12 col-lg-6">
                        <div class="work-process-wrap">
                            <div class="process-single-item">
                                <div class="process-icon-item left-shape">
                                    <div class="d-flex align-items-center">
                                        <div class="process-icon mr-4">
                                            <i class="fas fa-project-diagram color-primary"></i>
                                        </div>
                                        <div class="process-content text-left">
                                            <h5> انتخاب مربی و برنامه</h5>
                                            <p>از لیست مربیان ما , با توجه به رزومه ی آنها مناسب ترین گزینه برای هدف خودتون رو پیدا کنین</p>
                                        </div>
                                    </div>
                                    <svg x="0px" y="0px" width="312px" height="130px">
                                        <path class="dashed1" fill="none" stroke="rgb(95, 93, 93)" stroke-width="1" stroke-dasharray="1300" stroke-dashoffset="0" d="M311.000,0.997 C311.000,0.997 313.123,123.592 214.535,79.996 C214.535,79.996 41.149,20.122 3.377,125.996"></path>
                                        <path class="dashed2" fill="none" stroke="#ffffff" stroke-width="2" stroke-dasharray="6" stroke-dashoffset="1300" d="M311.000,0.997 C311.000,0.997 313.123,123.592 214.535,79.996 C214.535,79.996 41.149,20.122 3.377,125.996"></path>
                                    </svg>
                                </div>
                            </div>
                            <div class="process-single-item">
                                <div class="process-icon-item right-shape">
                                    <div class="d-flex align-items-center">
                                        <div class="process-icon ml-4">
                                            <i class="fas fa-puzzle-piece color-primary"></i>
                                        </div>
                                        <div class="process-content text-right">
                                            <h5>پر کردن پرسش نامه و پرداخت</h5>
                                            <p>بعد از انتخاب برنامه , پرسش نامه رو پر کنین و هزینه رو به صورت آنلاین پرداخت کنین</p>
                                        </div>
                                    </div>
                                    <svg x="0px" y="0px" width="312px" height="130px">
                                        <path class="dashed1" fill="none" stroke="rgb(95, 93, 93)" stroke-width="1" stroke-dasharray="1300" stroke-dashoffset="0" d="M3.121,2.028 C3.121,2.028 1.003,124.928 99.352,81.226 C99.352,81.226 272.319,21.200 310.000,127.338"></path>
                                        <path class="dashed2" fill="none" stroke="#ffffff" stroke-width="2" stroke-dasharray="6" stroke-dashoffset="1300" d="M3.121,2.028 C3.121,2.028 1.003,124.928 99.352,81.226 C99.352,81.226 272.319,21.200 310.000,127.338 "></path>
                                    </svg>
                                </div>
                            </div>
                            <div class="process-single-item">
                                <div class="process-icon-item left-shape mb-0">
                                    <div class="d-flex align-items-center">
                                        <div class="process-icon mr-4">
                                            <i class="fas fa-key color-primary"></i>
                                        </div>
                                        <div class="process-content text-left">
                                            <h5>دریافت برنامه</h5>
                                            <p>برنامه ی خودتونو با جزئیات و فیلم آموزشی تمرینات دریافت کنین</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <div class="img-wrap">
                            <img src="~/mainassets/img/choose.png" alt="roadmap" class="img-fluid">
                        </div>
                    </div>
                </div>
            </div>
        </section>


    </div>

    <!--footer section start-->
    <!--when you want to remove subscribe newsletter container then you should remove .footer-with-newsletter class-->
    <footer class="footer-1 gradient-bg ptb-60 footer-with-newsletter">

        <div class="container">
            <div class="row">
                <div class="col-md-12 col-lg-4 mb-4 mb-md-4 mb-sm-4 mb-lg-0">
                </div>
            </div>
        </div>
        <!--end of container-->
    </footer>

    <div class="scroll-top scroll-to-target primary-bg text-white" data-target="html">
        <span class="fas fa-hand-point-up"></span>
    </div>
    <!--scroll bottom to top button end-->
    <!--build:js-->
    <script src="~/mainassets/js/vendors/jquery-3.5.1.min.js"></script>
    <script src="~/mainassets/js/vendors/popper.min.js"></script>
    <script src="~/mainassets/js/vendors/bootstrap.min.js"></script>
    <script src="~/mainassets/js/vendors/jquery.easing.min.js"></script>
    <script src="~/mainassets/js/vendors/owl.carousel.min.js"></script>
    <script src="~/mainassets/js/vendors/countdown.min.js"></script>
    <script src="~/mainassets/js/vendors/jquery.waypoints.min.js"></script>
    <script src="~/mainassets/js/vendors/jquery.rcounterup.js"></script>
    <script src="~/mainassets/js/vendors/magnific-popup.min.js"></script>
    <script src="~/mainassets/js/vendors/validator.min.js"></script>
    <script src="~/mainassets/js/app.js"></script>
    <!--endbuild-->



</body>

</html>